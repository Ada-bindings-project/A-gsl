project Gsl is

   for Source_Dirs use ("src/**");
   for Object_Dir use ".obj";

   package Builder is
      for Default_Switches ("ada") use ("-k", "-j3", "-s");
   end Builder;

   package Compiler is
      for Default_Switches ("ada") use ("-gnatwa", "-gnatf", "-gnat05");
   end Compiler;

end Gsl;

